package k_Chapter10;

interface Flyable{
	int MAX_SPEED = 100;
	void fly();
	
	
	// 디폴트 메서드
	default void land() {
		System.out.println("Landing");
	}
	
	static int getWindCount() {
		return 2;
	}
}

class Bird implements Flyable{
	public void fly() {
		System.out.println("Bird is flying at speed " + MAX_SPEED);
	}
	@Override
	public void land() {
		System.out.println("....Landing.....");
	}
	
	
}
public class C_Interface02 {
		public static void main(String[] args) {
			Bird bird = new Bird();
			
			bird.fly();
		}
}
